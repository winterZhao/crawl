extends layout

block content
  .container
    .row
      .col-md-8
        ul.list-group
          each article in articles
            li.list-group-item
              span=article.id
              a(href=article.url,target='_blank')=article.name
      .col-md-4
        ul.list-group
          each category in categories
            li.list-group-item
              a(href=category.url)=category.name

